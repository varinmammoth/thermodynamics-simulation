#%%
import particle as p
import matplotlib.pyplot as plt

plt.plot(1,1) #used to initialise animation (IDE specific thing)
plt.show()

simulation = p.Simulation(p.container, p.ball2)

frames = 200 #number of iterations

simulation.run(frames, animate=False)
plt.plot(simulation._timeArray,simulation._pressureArray)
plt.show()

